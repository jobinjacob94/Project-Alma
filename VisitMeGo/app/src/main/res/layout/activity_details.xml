<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_details"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jobinjacob.visitmego.Details">

    <TextView
        android:text="VistMeGo!"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/visitmego"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="304dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="128dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="304dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        tools:layout_editor_absoluteY="322dp"
        tools:layout_editor_absoluteX="161dp"
        android:id="@+id/editText"
        android:hint="YOUR FULL NAME" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textEmailAddress"
        android:ems="10"
        tools:layout_editor_absoluteY="443dp"
        tools:layout_editor_absoluteX="161dp"
        android:id="@+id/editText2"
        android:hint="YOUR EMAIL ADDRESS" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        tools:layout_editor_absoluteY="545dp"
        tools:layout_editor_absoluteX="161dp"
        android:id="@+id/editText3"
        android:hint="COMPANY" />

    <AutoCompleteTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="657dp"
        tools:layout_editor_absoluteX="161dp"
        android:id="@+id/autoCompleteTextView2"
        android:hint="HOST" />

    <Button
        android:text="CONTINUE"
        android:layout_width="372dp"
        android:layout_height="94dp"
        tools:layout_editor_absoluteY="929dp"
        tools:layout_editor_absoluteX="202dp"
        android:id="@+id/continueButton"
        style="@style/Widget.AppCompat.Button.Colored" />
</android.support.constraint.ConstraintLayout>
